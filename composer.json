{
    "name": "first-iraqi-bank/fib-php-payment-sdk",
    "description": "SDK for integrating with the FIB payment system, enabling user authentication and payment transactions.",
    "version": "0.1.0",
    "type": "library",
    "keywords": [
        "sdk",
        "fib",
        "payment",
        "integration"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gateway ICT",
            "email": "info@the-gw.com",
            "homepage": "https://the-gw.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.5",
        "vlucas/phpdotenv": "5.6.x-dev"
    },
    "autoload": {
        "psr-4": {
            "FirstIraqiBank\\FIBPaymentSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FirstIraqiBank\\FIBPaymentSDK\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox"
    }
}
